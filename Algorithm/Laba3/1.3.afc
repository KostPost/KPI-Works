<!DOCTYPE AFC>
<algorithm version="1.2">
  <branch>
    <ou vars="введення ↋"/>
    <assign dest="sum" src="0"/>
    <assign dest="n" src="1"/>
    <io vars="x"/>
    <pre cond="sum &lt;+ ↋">
      <branch>
        <process text="sum += 2/(2n + 1)(2n + 3)"/>
        <process text="n += 1"/>
      </branch>
    </pre>
  </branch>
</algorithm>
