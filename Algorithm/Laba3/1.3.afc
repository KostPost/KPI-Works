<!DOCTYPE AFC>
<algorithm version="1.2">
  <branch>
    <assign src="0" dest="n"/>
    <assign src="0" dest="sum"/>
    <io vars="x"/>
    <pre cond="true">
      <branch>
        <process text="sum += 2/(2n + 1)(2n + 3)"/>
        <ou vars="Вивід Sum"/>
      </branch>
    </pre>
  </branch>
</algorithm>
